@model test2.Models.userName

@{
    ViewBag.Title = "Create";
    var u = Session["userName"] as test2.Models.userName;
}

<h2> @u.LastName, wecolcome to Google</h2>
<br />
<h2>Mail : @u.Email</h2>




@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
<div class="form-horizontal"style="float:left">
    <h4>userName</h4>
    <hr />

    <div class="form-group">
     
        <div class="col-md-10">
            
          
        </div>
    </div>
    <div class="form-group">
  
        <div class="col-md-10">
            @Html.EditorFor(model => model.Phone, new { htmlAttributes = new {placeholder = "Phone", @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.Phone, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">

        <div class="col-md-10">
            @Html.EditorFor(model => model.OldMail, new { htmlAttributes = new {placeholder = "Recovery Mail", @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.OldMail, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">

        <div class="col-md-10">
            @Html.EditorFor(model => model.Date, new { htmlAttributes = new { placeholder = "Date dd/mm/yy", @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.Date, "", new { @class = "text-danger" })
        </div>   
     
    </div>
 

    <div class="form-group">

        <div class="col-md-10">
            @Html.DropDownList("IDGenner", null, "Select Genner", htmlAttributes: new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.IDGenner, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Create" class="btn btn-info" />
        </div>
    </div>
</div>
    @ViewBag.Fail 
}
<img src="~/Image/gg2.png" width="300" height="350" style="float:left" />

